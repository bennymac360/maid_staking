{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "jsx": "react-jsx",
      "allowJs": true,
      "checkJs": false,
      "strict": false,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "skipLibCheck": true, 
      "isolatedModules": true,
      "noEmit": true // Since you're not compiling TypeScript files of your own, no output is necessary
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  